let SessionLoad = 1
if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
imap <F8> :make test
map <silent>  :if expand("%") == "":browse confirm w:else:confirm w:endif
vmap * :s/\/\///
map Q gq
vmap \ :s/^/\/\//
nmap gx <Plug>NetrwBrowseX
nmap <F11> :!make && konsole -e "gdb ./tst_group"
nmap <F12> :mkses! vimses :wqa
nmap <F8> :make test
nmap <F7> :make clean
nmap <F10> :!rxvt -e "/home/projects/absalg_root/absalg/gcc/blast.sh"
nmap <F9> :!konsole --workdir . --hold -e "make"
nmap <F3> :tabnew
nmap <F2> :tabnext
nmap <F1> :tabprevious
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetrwBrowseX(expand("<cWORD>"),0)
inoremap  u
abbr use1 //Uses:
abbr cmp1 //Computes:
abbr c2 *****************************************/
abbr c1 /*****************************************
let &cpo=s:cpo_save
unlet s:cpo_save
set autowrite
set background=dark
set backspace=2
set backup
set backupdir=/tmp
set backupskip=/tmp/*,/var/spool/cron/*
set cindent
set guifont=Courier\ 13
set guioptions=aegirLt
set helplang=en
set history=50
set iminsert=0
set incsearch
set nomodeline
set mouse=a
set ruler
set shiftwidth=4
set showcmd
set tabstop=4
set termencoding=utf-8
set whichwrap=b,s,<,>,h,l
set window=24
set nowritebackup
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd /home/projects/absalg_root/absalg/gcc
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 tst_group.cpp
badd +1 std_ex.h
badd +96 symmetric_rep.h
